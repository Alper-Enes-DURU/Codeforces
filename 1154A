#include <iostream>
using namespace std;
int main()
{
    int x, temp;
    int a, b, c;
    int  arr[4];    
    int maxValue = 0;
    for(int i = 0; i < 4; i++){
        cin >> x;
        arr[i] = x;
        for(int i=0;i<=10;i++)
        {
            for(int j=0;j<=10-i;j++)
            {
                if(arr[j] > arr[j+1])
                {
                temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
                }
            }
        }
    }
    a = (arr[0]-arr[1]+arr[2])/2;
    b = (arr[0] + arr[1] - arr[2])/2;
    c = (arr[2] - arr[0] + arr[1])/2;
    cout << a << b << c;
    return 0;
}
